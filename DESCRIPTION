Package: holiglm
Type: Package
Title: Holistic Generalized Linear Models
Version: 0.2.1
Authors@R: c(
    person("Benjamin", "Schwendinger", role = c("aut", "cre"), email = "benjaminschwe@gmail.com"),
    person("Florian", "Schwendinger", role = "aut"),
    person("Laura", "Vana", role = "aut")
